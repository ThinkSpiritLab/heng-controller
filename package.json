{
    "name": "heng-controller",
    "version": "0.0.1",
    "description": "",
    "author": "ThinkSpirit Developers",
    "private": true,
    "license": "MIT",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write src/*.ts src/**/*.ts test/*.ts test/**/*.ts",
        "check:format": "prettier --check src/*.ts src/**/*.ts test/*.ts test/**/*.ts",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint src test --fix",
        "check:lint": "eslint src test -f codeframe",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json"
    },
    "dependencies": {
        "@iarna/toml": "^2.2.5",
        "@nestjs/common": "^7.6.11",
        "@nestjs/core": "^7.0.0",
        "@nestjs/platform-express": "^7.0.0",
        "@nestjs/platform-ws": "^7.6.5",
        "@nestjs/websockets": "^7.6.5",
        "class-transformer": "^0.3.1",
        "class-validator": "^0.12.2",
        "commander": "^6.0.0",
        "eslint-plugin-prettier": "^3.3.1",
        "fs-extra": "^9.0.1",
        "generic-pool": "^3.7.1",
        "heng-protocol": "https://github.com/ThinkSpiritLab/Heng-Protocol",
        "ioredis": "^4.17.3",
        "lodash": "^4.17.20",
        "moment": "^2.29.1",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^6.5.4"
    },
    "devDependencies": {
        "@nestjs/cli": "^7.0.0",
        "@nestjs/schematics": "^7.0.0",
        "@nestjs/testing": "^7.0.0",
        "@types/commander": "^2.12.2",
        "@types/express": "^4.17.3",
        "@types/fs-extra": "^9.0.1",
        "@types/generic-pool": "^3.1.9",
        "@types/ioredis": "^4.17.5",
        "@types/jest": "25.2.3",
        "@types/lodash": "^4.14.159",
        "@types/moment": "^2.13.0",
        "@types/node": "^13.9.1",
        "@types/supertest": "^2.0.8",
        "@types/ws": "^7.4.0",
        "@typescript-eslint/eslint-plugin": "3.0.2",
        "@typescript-eslint/parser": "3.0.2",
        "eslint": "7.1.0",
        "eslint-config-prettier": "^6.10.0",
        "eslint-plugin-import": "^2.20.1",
        "jest": "26.0.1",
        "prettier": "^1.19.1",
        "supertest": "^4.0.2",
        "ts-jest": "26.1.0",
        "ts-loader": "^6.2.1",
        "ts-node": "^8.6.2",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^3.7.4"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
